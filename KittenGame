local Tab = Window:CreateTab("More", 4483362458)
local part

Tab:CreateToggle({
    Name = "Walk on water",
    Default = false,
    Callback = function(state)
        if state then
            if not part then
                part = Instance.new("Part")
                part.Position = Vector3.new(0, 7.75, 0)
                part.Size = Vector3.new(2048, 0.1, 2048)
                part.Anchored = true
                part.Transparency = 1
                part.Parent = game.Workspace
            end
        else
            if part then
                part:Destroy()
                part = nil
            end
        end
    end,
})
